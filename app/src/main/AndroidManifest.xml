<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruiz.customviewdemo">


    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ViewPositionActivity" />
        <activity android:name=".motionevent.MotionEventActivity" />
        <activity android:name=".gesturedetector.GestureDetectorActivity" />
        <activity android:name=".scroller.ScrollLinearLayoutActivity" />
        <activity android:name=".webmessageport.WebMessagePortActivity" />
        <activity android:name=".annotation.AnnotationActivity"></activity>
    </application>

</manifest>